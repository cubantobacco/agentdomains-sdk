{
  "name": "agentdomains-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Agent Native Domains â€” register domains with USDC via x402",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:coverage": "vitest run src/__tests__/index.test.ts --coverage --coverage.include=src/index.ts --coverage.reporter=text --coverage.reporter=json-summary",
    "test:coverage:gate": "npm run test:coverage && node scripts/check-coverage.mjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "domain",
    "registration",
    "x402",
    "usdc",
    "base",
    "ai-agent"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cubantobacco/agentdomains-sdk.git"
  },
  "homepage": "https://api.agentdomains.ai",
  "license": "MIT",
  "dependencies": {
    "@x402/fetch": "^2.3.0",
    "@x402/evm": "^2.3.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.4.0",
    "viem": "^2.0.0",
    "vitest": "^4.0.18"
  }
}
